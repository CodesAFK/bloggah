<% include ./partials/header %>


<% posts.forEach(function(post){ %>
  <h2><%= post.title %></h2>
    <img src="<%= post.image %>" alt="" height="175px" width="auto">
  <p><%= post.created %></p>
  <p><%= post.body.substring(0, 150) %>...<a href="/posts/<%= post._id %>">  Read More</a></p>

<% }); %>

<% include ./partials/footer %>